@foreach (var item in (List<EliteHospital.Data.APIRequestResponseModels.Response.Doctor>)ViewData["DoctorsList"])
    {
        var imgSrc = "";
        if (item.Photo != null)
        {
            var base64 = Convert.ToBase64String(item.Photo);
            imgSrc = String.Format("data:image/gif;base64,{0}", base64);
        }
        <div class="item">
            @if (item.Photo != null)
            {
                <img style="max-width: 100%; max-height: 100%;" src="@imgSrc" alt="">
            }
            else
            {
                <img style="max-width: 100%; max-height: 100%;" src="~/Content/MainSite/images/doctor-avatar-icon.png" alt="">
            }
            <img src="~/Content/MainSite/images/doctors-hm/osama.jpg" alt="">
            <div class="bk-apmt">
                <a class="bk-cpt bookappointment" href="#">Book An Appointment</a>
            </div>
            <div class="drtr-name">
                <p>@item.DoctorName</p>
                <small>
                    @item.DepartmentName
                    & @item.Designation
                </small>
            </div>
        </div>
    }
